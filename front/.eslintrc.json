{
  "parser":"babel-eslint",
  "parserOptions":{
    "ecmaVersion":"latest",
    "sourceType":"module",
    "ecmaFeatures":{
      "jsx":true
    }
  },
  "env":{
    "browser":true,
    "node":true,
    "es6":true
  },
  "plugin":["import", "react-hooks"],
  "extends":["airbnb"],
  "rules":{
    "jsx-a11y/label-has-associated-control":"off",
    "jsx-a11y/anchor-is-valide":"off",
    "no-console": "off",
    "no-underscore-dangle": "off", // var _foo;
    "react/forbid-prop-types":"off",
    "react/jsx-filename-extension":"off",
    "react/jsx-one-expression-per-line":"off",
    "obect-curly-newline":"off",
    "linebreak-style":"off",
    "no-param-reassign": "off"
  }
}